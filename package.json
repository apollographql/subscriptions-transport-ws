{
  "name": "subscriptions-transport-ws",
  "version": "0.6.0",
  "description": "A websocket transport for GraphQL subscriptions",
  "main": "dist/index.js",
  "browser": "dist/client.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/subscriptions-transport-ws.git"
  },
  "dependencies": {
    "@types/ws": "^0.0.41",
    "backo2": "^1.0.2",
    "eventemitter3": "^2.0.3",
    "graphql": "^0.10.0",
    "graphql-subscriptions": "^0.3.1",
    "graphql-tag": "^2.0.0",
    "iterall": "^1.1.1",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "ws": "^3.0.0"
  },
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "npm run testonly --",
    "posttest": "npm run lint",
    "lint": "tslint --format=stylish --type-check --project ./tsconfig.json",
    "watch": "tsc -w",
    "testonly": "mocha --reporter spec --full-trace ./dist/test/**/*.js",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover _mocha -- --full-trace ./dist/test/tests.js",
    "postcoverage": "remap-istanbul --input coverage/coverage.raw.json --type lcovonly --output coverage/lcov.info",
    "browser-compile": "webpack --config \"./unpkg-webpack.config.js\"",
    "prepublish": "npm run compile && npm run browser-compile"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/graphql": "^0.9.0",
    "@types/lodash": "4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/sinon": "^2.1.3",
    "chai": "^3.5.0",
    "istanbul": "^1.0.0-alpha.2",
    "lodash": "^4.17.1",
    "mocha": "^3.3.0",
    "remap-istanbul": "^0.9.5",
    "sinon": "^2.1.0",
    "tslint": "^5.1.0",
    "typescript": "^2.3.0",
    "webpack": "^2.4.1"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
