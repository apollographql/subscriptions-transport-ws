{
  "name": "subscriptions-transport-ws",
  "version": "0.5.1",
  "description": "A websocket transport for GraphQL subscriptions",
  "main": "dist/server.js",
  "browser": "dist/client.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/subscriptions-transport-ws.git"
  },
  "dependencies": {
    "@types/ws": "0.0.37",
    "backo2": "^1.0.2",
    "eventemitter3": "^2.0.2",
    "graphql-subscriptions": "^0.2.0",
    "graphql-tag": "^1.2.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "ws": "^1.1.1"
  },
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "npm run testonly --",
    "posttest": "npm run lint",
    "lint": "tslint ./src/**/*.ts",
    "watch": "tsc -w",
    "testonly": "mocha --reporter spec --full-trace ./dist/test/tests.js",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover _mocha -- --full-trace ./dist/test/tests.js",
    "postcoverage": "remap-istanbul --input coverage/coverage.raw.json --type lcovonly --output coverage/lcov.info",
    "browser-compile": "webpack",
    "prepublish": "npm run compile && npm run browser-compile"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/graphql": "^0.8.5",
    "@types/lodash": "^4.14.34",
    "@types/mocha": "^2.2.31",
    "@types/node": "^6.0.38",
    "@types/sinon": "^1.16.31",
    "chai": "^3.5.0",
    "graphql": "^0.7.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.0.0",
    "remap-istanbul": "^0.7.0",
    "sinon": "^1.17.6",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "webpack": "^1.14.0",
    "graphql-subscriptions": "^0.2.2",
    "json-loader": "^0.5.4",
    "ts-loader": "^1.3.3"
  },
  "peerDependencies": {
    "graphql-subscriptions": "^0.2.0"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "browser": "browser/bundle.js",
  "license": "MIT"
}
